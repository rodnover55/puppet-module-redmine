default:
  email_delivery:
    delivery_method: :async_smtp
    async_smtp_settings: 
      tls: true
      enable_starttls_auto: true
      address: <%= scope.lookupvar('redmine::smtp') %>
      port: <%= scope.lookupvar('redmine::smtp_port') %>
      domain: <%= scope.lookupvar('redmine::domain') %>
      authentication: :login
      user_name: "<%= scope.lookupvar('redmine::smtp_user') %>"
      password: "<%= scope.lookupvar('redmine::smtp_password') %>"
      openssl_verify_mode: "none"

  attachments_storage_path:

  autologin_cookie_name:
  autologin_cookie_path:
  autologin_cookie_secure:

  scm_subversion_command:
  scm_mercurial_command:
  scm_git_command:
  scm_cvs_command:
  scm_bazaar_command:
  scm_darcs_command:

